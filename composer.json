{
    "name": "nzolt/liveh",
    "type": "project",
    "description": "Workforce date to hours.",
    "keywords": [
        "Test project",
        "no framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "bramus/router": "~1.4",
        "jenssegers/blade": "^1.2",
        "nette/http": "^3.0",
        "vlucas/phpdotenv": "^5.3",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
